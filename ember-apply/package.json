{
  "name": "ember-apply",
  "version": "2.10.1",
  "description": "automatic integration and configuration from the community",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/ember-apply",
    "type": "https",
    "directory": "packages/ember-apply"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./declarations/src/index.d.ts"
    },
    "./test-utils": "./src/test-utils.js"
  },
  "main": "src/index.js",
  "type": "module",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": false,
  "bin": {
    "ember-apply": "src/cli/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./declarations/src/*",
        "./declarations/src/index.d.ts"
      ]
    }
  },
  "types": "./declarations/src/",
  "files": [
    "src/**/*",
    "declarations/**/*",
    "README.md"
  ],
  "scripts": {
    "prepack": "pnpm build && cp ../README.md ./",
    "build": "tsc --build",
    "lint:types": "tsc",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "test": "vitest --coverage --no-watch --no-threads",
    "test:watch": "vitest --watch --no-threads"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "chalk": "^5.3.0",
    "ember-template-recast": "^6.1.4",
    "execa": "^8.0.1",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "jscodeshift": "^0.15.0",
    "latest-version": "^7.0.0",
    "ora": "^6.3.1",
    "pacote": "^15.2.0",
    "postcss": "^8.4.31",
    "posthtml": "^0.16.5",
    "posthtml-boolean-attributes": "^0.3.1",
    "prettier": "^3.0.3",
    "semver": "^7.5.4",
    "sort-object-keys": "^1.1.3",
    "unified": "^11.0.0",
    "yargs": "^17.7.2",
    "yarn-workspaces-list": "^0.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@nullvoxpopuli/eslint-configs": "^3.2.2",
    "@types/fs-extra": "^11.0.2",
    "@types/jscodeshift": "^0.11.7",
    "@types/node": "^20.8.4",
    "@types/pacote": "^11.1.6",
    "@types/semver": "^7.5.3",
    "@types/sort-object-keys": "^1.1.1",
    "@types/yargs": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/ui": "^0.34.6",
    "c8": "^8.0.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-plugin-decorator-position": "^5.0.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.0",
    "typescript": "5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "volta": {
    "extends": "../package.json"
  }
}
